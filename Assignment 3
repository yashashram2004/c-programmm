#include <stdio.h>
int factorial(int n)
{
    int i, f=1;
    for(i=1;i<=n;i++) {
        f*=i;
    }
    return f;
}
int main()
{
    int n;
    printf("Enter the number: ");
    scanf("%d", &n);
    if(n == 1) {
        printf("Factorial of %d is %d", n,n);
    } else {
        printf("Factorial of %d is %d", n, factorial(n));
    }
    return 0;
}
